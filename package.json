{
  "name": "q-menu",
  "version": "1.0.0",
  "description": "A Responsive Menu for web apps and web sites build with StencilJS",
  "keywords": [
    "menu",
    "sub menu",
    "side menu",
    "stencil menu",
    "menu web component"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^1.3.0",
    "@stencil/sass": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteocusinato/q-menu.git"
  },
  "author": "Matteo Cusinato",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matteocusinato/q-menu"
  },
  "dependencies": {
    "@stencil/sass": "^1.0.1"
  }
}
